{
  "$schema": "https://raw.githubusercontent.com/rojo-rbx/rojo/master/default.project.json",
  "name": "rbxvamp",
  "tree": {
    "$className": "DataModel",
    "ServerScriptService": {
      "src": {
        "$path": "src"
      },
      "server": {
        "$path": "server"
      },
      "GameRunner": {
        "$path": "ServerScriptService/GameRunner.server.lua"
      },
      "MainServer": {
        "$path": "ServerScriptService/MainServer.server.lua"
      }
    },
    "ReplicatedStorage": {
      "assets": {
        "$path": "assets"
      },
      "src": {
        "$path": "src",
        "$ignore": ["**/*.server.lua"]
      }
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "ClientGameRunner": {
          "$path": "StarterPlayerScripts/ClientGameRunner.client.lua"
        }
      }
    },
    "StarterGui": {
      "UILoader": {
        "$path": "StarterGui/UILoader.client.lua"
      }
    }
  }
}
